        code : |1+
         
         bCRManagement is boolean
         
         // Retrieves the cursor position
         nCursor is int = EDT_SQL_QUERY..Cursor
         
         // Retrieves the SQL code
         bufCode    is Buffer = RTFToText(EDT_SQL_QUERY)
         
         // If the input cursor is found at the end of the code
         IF nCursor = Length(bufCode) THEN
         	
         	// Reposition the cursor while taking the carriage returns into account
         	bCRManagement = True
         END
         
         // Defines the SQL code
         SQL.SetCode(bufCode)
         
         // Performs the coloring
         EDT_SQL_QUERY = SQL.Coloring()
         
         // Repositions the input cursor
         EDT_SQL_QUERY..Cursor = nCursor + (bCRManagement ? Max(0,(StringCount(bufCode, CR) - 1)) ELSE 0)
