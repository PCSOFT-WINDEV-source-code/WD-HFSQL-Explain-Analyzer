      // Summary: Displays the result of EXPLAIN
      // Syntax:
      //DisplayOrganizationChart (<sExplainXML> is string)
      //
      // Parameters:
      //	sExplainXML (ANSI string): XML result of Explain
      // Return value:
      // 	None
      //
      PROCEDURE DisplayOrganizationChart(LOCAL sExplainXML is string)
      
      // Variables
      xmlExplain is a xmlDocument, description="explain"
      
      // Opens the XML
      xmlExplain = XMLOpen(sExplainXML,fromString)
      
      // Reset the organization chart
      OrgDeleteAll(ORGA_EXPLAIN)
      
      // Parses the XML
      _DisplayNode(xmlExplain.explain..ChildNode[1])
      
      // Displays the result
      ORGA_EXPLAIN..Visible    = True
      STC_EXPLANATION..Visible = False
     type : 458752
   -
     name : _DisplayNode
     internal_properties : 
     procedure_id : 1607142149068285289
     type_code : 14
     code : |1+
      // Summary: Displays a node of the XML of EXPLAIN
      // Syntax:
      //_DisplayNode (<xmlExplainNode> is xmlNode [, <nParentSubscript> is int])
      //
      // Parameters:
      //	xmlExplainNode (xmlNode): Node of the XML of EXPLAIN
      //	nParentSubscript (integer - default value=-1): Subscript of the parent element
      // Return value:
      // 	None
      //
      PROCEDURE PRIVATE _DisplayNode(xmlExplainNode is a xmlNode, nParentSubscript is int = -1)
      
      nNewSubscript is int
      sType         is string = xmlExplainNode..Attribute["type"]..Value
      xmlChildNode  is a xmlNode
      sName          is string
      tmpElement    is an OrgElement
      bStops is boolean
      
      IF NOT sType IN ("materialize","aggregate") THEN
      	IF nParentSubscript = -1 THEN
      		nNewSubscript = OrgAdd(ORGA_EXPLAIN,sType)
      	ELSE
      		nNewSubscript = OrgAddChild(ORGA_EXPLAIN,nParentSubscript,sType)	
      	END
      END
      
      IF nNewSubscript > 0 THEN
      	// Stores the type
      	tmpElement.Note = sType
      	tmpElement.BrushColor = White
      	
      	// According to the type of plane, there is information to display
      	SWITCH sType
      		CASE "table"
      			tmpElement.Title +=" " + xmlExplainNode.name..Text
      			IF xmlExplainNode."index-scan"..Attribute["key"] <> "" THEN
      				tmpElement.Content = "Key used: " + xmlExplainNode."index-scan"..Attribute["key"]
      			END
      			IF xmlExplainNode.filter..Text = "" THEN
      				tmpElement.Content += [CR] + "Full browse of the file"
      				tmpElement.BrushColor = PastelRed
      			ELSE							
      				tmpElement.Content += [CR] + "Filter: " + xmlExplainNode.filter..Text
      			END
      			bStops = True
      		CASE "join"
      			tmpElement.Title = "Join"
      			tmpElement.Content = "Method: " + xmlExplainNode."join-method"..Text 
      			tmpElement.Content += [CR] + "Type: " +  xmlExplainNode."join-type"..Text	
      			
      		CASE "union"
      			// No action
      			tmpElement.Title = "Union"
      		CASE "materialize"
      			// No action
      			
      		CASE "sort"
      			tmpElement.Title = "Sort"
      			tmpElement.Content = "Sorted item(s): " 
      			sItems is string
      			
      			FOR i = 1 _TO_ xmlExplainNode."sort-key"..ChildNode..Occurrence
      				sItems += [", "] + xmlExplainNode."sort-key"..ChildNode[i]..Text
      			END
      			
      			tmpElement.Content += sItems
      			
      		CASE "distinct"
      			// No action
      			
      		CASE "limit"
      			
      			tmpElement.Title = "Limit"
      			
      			tmpElement.Content = "Type: " + xmlExplainNode.type..Text
      			tmpElement.Content += [CR] + "Nb Lines: " + xmlExplainNode.rows..Text		
      			tmpElement.Content += [CR] + "offset = " + Val(xmlExplainNode.offset..Text)
      			IF xmlExplainNode."rows-percent"..Text = "true" THEN
      				tmpElement.Content += [CR] + "Percentage: Yes"
      			ELSE
      				tmpElement.Content += [CR] + "Percentage: No"				
      			END
      			
      		CASE "filter"
      			tmpElement.Content = xmlExplainNode.filter..Text
      			
      			
      		OTHER CASE:
      			dbgAssert(False,"Type of plane not supported")
      	END
      	
      	// Updates the added element
      	ORGA_EXPLAIN[nNewSubscript] =  tmpElement
      ELSE
      	nNewSubscript = nParentSubscript
      END
      
      IF bStops = True THEN
      	RETURN
      END
      
      // Continue to browse the child by adding the child planes
      FOR EACH xmlChildNode of xmlExplainNode..ChildNode
      	sName = xmlChildNode..Name
      	IF sName = "plan" THEN		
      		_DisplayNode(xmlChildNode,nNewSubscript)	
      	END
      END
      
