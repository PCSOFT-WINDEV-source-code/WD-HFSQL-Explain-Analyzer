   -
     name : CheckClientServerInput
     procedure_id : 1607142149068088681
     type_code : 14
     code : |1-
      // Summary: Checks the input of HFSQL Client/Server controls
      // Syntax:
      //[ <Result> = ] CheckClientServerInput ([<bCheckDB> is boolean])
      //
      // Parameters:
      // 	bCheckDB (boolean - default value=1): True if the database must be checked
      // Return value:
      // 	boolean: True if the data is correct
      //
      PROCEDURE CheckClientServerInput(LOCAL bCheckDB is boolean = True)
      
      IF EDT_SERVER ~= "" THEN
      	Error(<§$0000§>)
      	ReturnToCapture(EDT_SERVER)	
      END
      
      IF Val(EDT_PORT) = 0 THEN
      	Error(<§$0001§>)
      	ReturnToCapture(EDT_PORT)
      END
      
      IF EDT_USER ~= "" THEN
      	Error(<§$0002§>)
      	ReturnToCapture(EDT_USER)
      END
      
      IF bCheckDB THEN
      	IF EDT_DB ~= "" THEN
      		Error(<§$0003§>)
      		ReturnToCapture(EDT_DB)
      	END
      END
      
      RESULT True
     type : 458752
