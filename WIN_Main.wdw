      // Summary: Closes the connection to the server
      // Syntax:
      //_CloseClientServerConnection ()
      //
      PROCEDURE PRIVATE _CloseClientServerConnection()
      
      // Variables
      sListFile is string
      sFileName   is string
      
      // Lists the files
      sListFile = HListFile(gcntConnection)
      
      // Declares the files
      FOR EACH string sFile of sListFile SEPAREE by CR
      	
      	// Extracts the name of the file
      	sFileName = fExtractPath(sFile,fFileName)
      	HCancelDeclaration(sFileName)
      END
      
      HCloseConnection(gcntConnection)
      
      CASE Error:
      ErrorPropagate()
      CASE EXCEPTION:
      ExceptionPropagate()
