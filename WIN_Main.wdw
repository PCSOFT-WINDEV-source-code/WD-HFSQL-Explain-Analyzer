      // Summary: Establishes the Client/Server connection
      // Syntax:
      //_OpenClientServerConnection ()
      
      PROCEDURE PRIVATE _OpenClientServerConnection()
      
      // Variables
      sListFile is string
      sFileName   is string
      aaFiles    is ASSOCIATIVE ARRAY of strings
      
      // Describe the connection 
      gcntConnection..User    = EDT_USER 
      gcntConnection..Password     = EDT_SERVER_PASSWORD 
      gcntConnection..Server        = EDT_SERVER+":"+EDT_PORT 
      gcntConnection..Database  = EDT_DB 
      gcntConnection..Provider       = hAccessHFClientServer 
      gcntConnection..Access          = hOReadWrite 
      gcntConnection..CursorOptions = hClientCursor 
      
      // Opens the connection to the server 
      HOpenConnection(gcntConnection)
      
      // Lists the files
      sListFile = HListFile(gcntConnection)
      
      // Declares the files
      FOR EACH string sFile of sListFile SEPAREE by CR
      	
      	// Extracts the name of the file
      	sFileName = fExtractPath(sFile,fFileName)
      	
      	// Already declared?
      	IF aaFiles[sFileName]..Empty THEN
      		// Declares the file in order to run the SQL query
      		HPass(sFileName,EDT_SERVER_HFFILE_PASSWORD)
      		IF HDeclareExternal(sFile,sFileName,gcntConnection) = False THEN
      			HErrorPassword()
      			CONTINUE
      		END
      		
      		aaFiles[sFileName] = sFileName
      	END	
      END
      
      CASE Error:
      ErrorPropagate()
      CASE EXCEPTION:
      ExceptionPropagate()
