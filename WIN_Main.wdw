        code : |1-
         
         // Variables
         dsExplain    is data source
         sXML         is string
         GR_DETAILS..Visible = False
         // Checks the input
         IF CheckInput() = True THEN
         	
         	// SQL query?
         	IF RTFToText(EDT_SQL_QUERY) ~= "" THEN
         		Error(<§$0005§>)
         		ReturnToCapture(EDT_SQL_QUERY)	
         	END
         	
         	// Opens the connection
         	OpenConnection()
         	
         	// Runs the SQL query
         	IF HExecuteSQLQuery(dsExplain,"EXPLAIN "+RTFToText(EDT_SQL_QUERY)) = False THEN
         		Error(HErrorInfo())
         		ReturnToCapture(EDT_SQL_QUERY)
         	END
         	
         	// Reads the first record to retrieve the XML
         	HReadFirst(dsExplain)
         	sXML = dsExplain.EXPLAIN
         	
         	// Builds the organization chart from the XML
         	DisplayOrganizationChart(sXML)
         	clXMLColoring is cXMLColoring(XMLToText(UTF8ToString(sXML)))
         	clXMLColoring.SetOutputMode(cXMLColoring.OUT_RTF)
         	
         	EDT_XML = clXMLColoring.Coloring()
         END
         
         CASE EXCEPTION:
         Error(ExceptionInfo())
         END:
         // Closes the connection
         CloseConnection()
